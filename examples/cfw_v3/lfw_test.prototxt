name: "lfw_test"

layers {
  name: "data_a"
  type: IMAGE_DATA
  top: "data_a"
  top: "label_a"
  image_data_param {
    source: "data/lfw/lfw_tst_a.txt"
    batch_size: 64
    shuffle: false
  }
  transform_param {
    mean_file: "data/cfw/mean.binaryproto"
    scale: 0.00390625
  }
}

layers {
  name: "data_b"
  type: IMAGE_DATA
  top: "data_b"
  top: "label_b"
  image_data_param {
    source: "data/lfw/lfw_tst_b.txt"
    batch_size: 64
    shuffle: false
  }
  transform_param {
    mean_file: "data/cfw/mean.binaryproto"
    scale: 0.00390625
  }
}


layers {
  type: IMPORT
  name: "a"
  import_param {
    net: "examples/cfw_v3/shared.prototxt"
    var { name: "bottom" value: "/data_a" }
    var { name: "label" value: "/label_a" }
  }
}

layers {
  type: IMPORT
  name: "b"
  import_param {
    net: "examples/cfw_v3/shared.prototxt"
    var { name: "bottom" value: "/data_b" }
    var { name: "label" value: "/label_b" }
  }
}

layers {
  type: IMPORT
  name: "same_label"
  import_param {
    net: "examples/cfw_v3/same_label.prototxt"
  }
}

layers {
  type: IMPORT
  name: "contrast"
  import_param {
    net: "examples/cfw_v3/contrast.prototxt"
  }
}

